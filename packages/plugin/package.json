{
  "name": "@expressive-tea/plugin",
  "version": "1.0.2",
  "description": "A Plugin Engine for Expressive Tea Framework.",
  "scripts": {
    "build:dev": "tsc --project tsconfig.json --watch",
    "build": "tsc --project tsconfig.json",
    "clean": "trash '**/*.js' '**/*.d.ts' '**/*.js.map' '**/*.d.ts.map' '!node_modules/**/*'",
    "linter": "tslint -c ../../tslint.json -p tsconfig.json",
    "publish:prepare": "npm run clean && npm run build",
    "postpublish": "npm run clean",
    "prepublishOnly": "npm run linter && npm run publish:prepare"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Expressive-Tea/packages"
  },
  "bugs": {
    "url": "https://github.com/Expressive-Tea/packages/issues",
    "email": "projects@expressive-tea.io"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Diego Resendez <diego.resendez@zero-oneit.com> (https://github.com/zerooneit)"
  ],
  "keywords": [
    "Expressive-Tea",
    "plugin",
    "typescript",
    "decorators"
  ],
  "author": "Expressive Tea <projects@expressive-tea.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@expressive-tea/commons": "^1.0.0"
  }
}
